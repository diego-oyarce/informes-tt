%Formate de citas para usar como guía:
%Para libros usar:
@book{nombreclave,
  author = {Apellidos, Nombres},
  title = {Título del libro},
  publisher = {Editorial},
  year = {Año de publicación},
  isbn = {Número ISBN (opcional)}
}

%Para artículo de revista usar:
@article{nombreclave,
  author = {Apellidos, Nombres},
  title = {Título del artículo},
  journal = {Nombre de la revista},
  volume = {Volumen},
  number = {Número},
  pages = {Páginas},
  year = {Año de publicación},
  doi = {DOI (opcional)}
}

%Para sitios web usar:
@misc{nombreclave,
  author = {Autor(es) (si está disponible)},
  title = {Título de la página web},
  howpublished = {Sitio web},
  year = {Año de acceso},
  url = {URL de la página web},
  note = {Notas adicionales (opcional)}
}

%Para un paper usar:
@unpublished{nombreclave,
  author = {Apellido, Nombre},
  year = {año},
  title = {Título del working paper},
  institution = {Nombre de la institución o sitio web},
  note = {Tipo de documento},
}

%Para una tesis
@phdthesis{ApellidoAutor,
  author = {Apellido, Autor},
  year = {año},
  title = {Título de la tesis},
  school = {Nombre de la institución},
}

